<h1>Create New Question for Test <%= @test.title %></h1>

<%= form_with(model: [@test, @question], local: true) do |form| %>
  <div class="field">
    <%= form.label :body, "Question text:" %>
    <%= form.text_area :body %>
  </div>

  <div class="actions">
    <%= form.submit "Create Question" %>
  </div>
<% end %>
