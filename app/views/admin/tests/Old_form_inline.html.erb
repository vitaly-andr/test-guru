<%= turbo_frame_tag(dom_id(test)) do %>
  <%= form_with model: [:admin, test], url: update_inline_admin_test_path(test), local: true, class: 'form-inline' do |form| %>
    <%= form.text_field :title, class: 'form-control mr-2' %>
    <%= form.submit t('helpers.submit.test.update_inline'), class: 'btn btn-primary' %>
  <% end %>
<% end %>
