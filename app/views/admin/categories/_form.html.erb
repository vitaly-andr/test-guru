<%= form_with model: [:admin, category] do |form| %>
  <h1><%= form_header(category) %></h1>
  <%= render 'shared/errors', resource: category %>
  <div class="field">
    <%= form.label :title %>
    <%= form.text_field :title %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
